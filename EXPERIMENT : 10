#include <stdio.h>

#define MAX 10

int adj[MAX][MAX];
int visited[MAX];
int n;

void DFS(int v) {
    int i;
    visited[v] = 1;
    printf("%d ", v);

    for (i = 0; i < n; i++) {
        if (adj[v][i] == 1 && visited[i] == 0)
            DFS(i);
    }
}

void BFS(int start) {
    int queue[MAX], front = 0, rear = 0;
    int i, current;

    for (i = 0; i < n; i++)
        visited[i] = 0;

    visited[start] = 1;
    queue[rear++] = start;

    printf("%d ", start);

    while (front != rear) {
        current = queue[front++];

        for (i = 0; i < n; i++) {
            if (adj[current][i] == 1 && visited[i] == 0) {
                printf("%d ", i);
                visited[i] = 1;
                queue[rear++] = i;
            }
        }
    }
}

int main() {
    int i, j, edges, v1, v2, start;

    printf("Enter the number of cities (vertices): ");
    scanf("%d", &n);

    // Initialize adjacency matrix
    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            adj[i][j] = 0;
        }
    }

    printf("Enter the number of edges: ");
    scanf("%d", &edges);

    printf("Enter the edges (Format: city1 city2):\n");
    for (i = 0; i < edges; i++) {
        scanf("%d%d", &v1, &v2);
        adj[v1][v2] = adj[v2][v1] = 1; // Undirected graph
    }

    printf("\nAdjacency Matrix:\n");
    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++)
            printf("%d ", adj[i][j]);
        printf("\n");
    }

    printf("\nEnter the starting city (0 to %d): ", n - 1);
    scanf("%d", &start);

    printf("\nDFS Traversal starting from city %d:\n", start);
    for (i = 0; i < n; i++)
        visited[i] = 0;
    DFS(start);

    printf("\n\nBFS Traversal starting from city %d:\n", start);
    BFS(start);

    printf("\n");

    return 0;
}
